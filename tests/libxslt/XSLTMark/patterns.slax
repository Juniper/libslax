version 1.2;

output-method {
    encoding "utf-8";
}

match table/row[6] {
    priority 5;
    
    <row6> {
        apply-templates;
    }
}

match table/row[id == "0091"] {
    <id91> {
        apply-templates;
    }
}

match //row[firstname == "Egon"] {
    <egon> {
        apply-templates;
    }
}

match table//row[*[. == "Aranow"]] {
    <aranow> {
        apply-templates;
    }
}

match row {
    priority -1;
    
    <row> {
        apply-templates;
    }
}

match row[id mod 3 == 2] [following-sibling::row[4]/firstname == "Bob"] {
    <crazy> {
        apply-templates;
    }
}

match id | firstname | lastname | street | city | state | zip {
    copy-node {
        expr .;
    }
}
