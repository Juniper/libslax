version 1.2;

output-method {
    encoding "utf-8";
}

match top {
    <TOP> {
        apply-templates;
    }
}

match bar[baz || following-sibling::*[1][self::nar]] {
    <BAR> .;
}

match foo[following-sibling::*[position() <= 2][self::barg] && following-sibling::*[position() <= 2][self::nar]] {
    <FOO> .;
}
