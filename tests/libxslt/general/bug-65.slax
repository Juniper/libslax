version 1.2;

ns libxslt exclude = "http://xmlsoft.org/XSLT/namespace";

output-method xml {
    indent "yes";
}

var $one = {
    copy-of .;
}
var $two = document("bug-65.ent");

main {
    apply-templates $two {
        mode "two";
    }
}

match * {
    mode "two";
    
    for-each (//content) {
        apply-templates libxslt:node-set($one) {
            mode "one";
        }
        expr "\n-----------------\n";
    }
}

match * {
    mode "one";
    
    for-each (//content) {
        /* here is the problem (.//content works well, but I need the key
        function) */
        copy-of .;
    }
}
