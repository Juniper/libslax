version 1.2;

ns "http://www.w3.org/1999/xhtml";

output-method xml {
    version "1.0";
    indent "yes";
    encoding "US-ASCII";
    doctype-public "-//W3C//DTD XHTML 1.0 Strict//EN";
    doctype-system "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd";
}

main <html> {
    <head> {
        <style type="text/css" media="all"> '@import\n"pretty_xhtml.css";';
    }
    <body> {
        <h1> "foo";
    }
}
